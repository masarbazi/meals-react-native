{
  "expo": {
    "name": "meals-react-native",
    "slug": "meals-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app/meals-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/app/splash.png",
